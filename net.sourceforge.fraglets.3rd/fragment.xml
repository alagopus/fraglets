<?xml version="1.0" encoding="UTF-8"?>
<fragment
   id="net.sourceforge.fraglets.3rd"
   name="3rd party software"
   version="0.0.1"
   provider-name="fraglets.sourceforge.net"
   plugin-id="net.sourceforge.fraglets.zeig.core"
   plugin-version="0.0.1"
   match="compatible">

   <runtime>
      <library name="3rd/jaf-1.0.2/activation.jar">
         <export name="*"/>
         <packages prefixes="javax.activation,com.sun.activation"/>
      </library>
      <library name="3rd/mysql-connector-java-3.0.6-stable/mysql-connector-java-3.0.6-stable-bin.jar">
         <export name="*"/>
         <packages prefixes="com.mysql.jdbc,org.gjt.mm.mysql"/>
      </library>
      <library name="3rd/xalan-j_2_5_0/bin/xalan.jar">
         <export name="*"/>
         <packages prefixes="org.apache.xpath,org.apache.xml,org.apache.xalan"/>
         <packages prefixes="org.apache.regexp,org.apache.bcel,org.w3c.dom.xpath"/>
         <packages prefixes="java_cup,JLex"/>
      </library>
      <library name="3rd/xalan-j_2_5_0/bin/bsf.jar">
         <export name="*"/>
         <packages prefixes="com.ibm.bsf"/>
      </library>
   </runtime>


</fragment>
